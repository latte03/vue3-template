    "unplugin-vue-inspector": "^0.2.2",
    "unplugin-vue-macros": "^1.9.1",
    "unplugin-vue-router": "^0.5.1",
    "vite": "^4.1.4",
    "vite-plugin-pages": "^0.28.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-layouts": "^0.8.0",
