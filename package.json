    "axios": "^1.3.4",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.32",
    "tdesign-icons-vue-next": "^0.1.7",
    "tdesign-vue-next": "^1.1.1",
    "unocss": "^0.48.5",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^20.0.1",
