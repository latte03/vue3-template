{
  "name": "vue3-template",
  "version": "0.1.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "concurrently \"pnpm:type-check\" \"pnpm:build-only\"",
    "preview": "vite preview",
    "test": "vitest",
    "test:unit": "vitest --environment jsdom --root src/",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "eslint-viewer": "eslint-flat-config-viewer",
    "lint": "eslint . --fix",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "cz": "git add . && cz"
  },
  "dependencies": {
    "@unocss/reset": "^0.62.3",
    "@vue-macros/reactivity-transform": "^1.1.0",
    "@vueuse/core": "^11.0.3",
    "@vueuse/head": "^2.0.0",
    "alova": "^3.0.16",
    "axios": "^1.7.7",
    "fast-querystring": "^1.1.2",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.2.2",
    "tdesign-icons-vue-next": "^0.2.6",
    "tdesign-vue-next": "^1.10.1",
    "unocss": "^0.62.3",
    "vue": "^3.5.5",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@plugin-web-update-notification/vite": "^1.7.1",
    "@sxzz/eslint-config": "^4.2.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.5.5",
    "@types/nprogress": "^0.2.3",
    "@unocss/preset-rem-to-px": "^0.62.3",
    "@unocss/transformer-directives": "^0.62.3",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue-macros/volar": "^0.30.1",
    "@vue/runtime-core": "^3.5.5",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "commitizen": "^4.3.0",
    "commitlint": "^19.5.0",
    "concurrently": "^9.0.1",
    "cz-customizable": "^7.2.1",
    "eslint": "^9.10.0",
    "eslint-define-config": "^2.1.0",
    "eslint-flat-config-viewer": "^0.1.20",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "jsdom": "^25.0.0",
    "less": "^4.2.0",
    "pnpm-patch-i": "^0.4.1",
    "postcss": "^8.4.45",
    "postcss-html": "^1.7.0",
    "postcss-less": "^6.0.0",
    "prettier": "^3.3.3",
    "stylelint": "^16.9.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^5.1.0",
    "stylelint-config-recommended": "^14.0.1",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-declaration-strict-value": "^1.10.6",
    "terser": "^5.32.0",
    "typescript": "~5.6.2",
    "unplugin-auto-import": "^0.18.3",
    "unplugin-compression": "^0.0.6",
    "unplugin-info": "^1.1.0",
    "unplugin-svg-component": "^0.10.3",
    "unplugin-vue-components": "^0.27.4",
    "unplugin-vue-inspector": "^2.2.0",
    "unplugin-vue-macros": "^2.12.0",
    "unplugin-vue-router": "^0.10.8",
    "vite": "^5.4.5",
    "vite-plugin-vue-meta-layouts": "^0.4.3",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.1",
    "volar-plugin-vue-router": "^0.2.0",
    "vue-tsc": "^2.1.6"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "config": {
    "cz-customizable": {
      "config": "./.cz-config.cjs"
    },
    "commitizen": {
      "path": "cz-customizable"
    }
  }
}
