{
  "name": "vue3-template",
  "version": "0.1.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "concurrently \"pnpm:type-check\" \"pnpm:build-only\"",
    "preview": "vite preview",
    "test": "vitest",
    "test:unit": "vitest --environment jsdom --root src/",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "eslint-viewer": "eslint-flat-config-viewer",
    "lint": "eslint . --fix",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "cz": "git add . && cz"
  },
  "dependencies": {
    "@unocss/reset": "^0.58.0",
    "@vue-macros/reactivity-transform": "^0.4.0",
    "@vueuse/core": "^10.7.0",
    "@vueuse/head": "^2.0.0",
    "alova": "^2.15.1",
    "axios": "^1.6.2",
    "fast-querystring": "^1.1.2",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "tdesign-icons-vue-next": "^0.2.2",
    "tdesign-vue-next": "^1.7.0",
    "unocss": "^0.58.0",
    "vue": "^3.3.10",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@sxzz/eslint-config": "^3.7.5",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.10.3",
    "@types/nprogress": "^0.2.3",
    "@unocss/preset-rem-to-px": "^0.58.0",
    "@unocss/transformer-directives": "^0.58.0",
    "@vitejs/plugin-vue": "^4.5.1",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue-macros/volar": "^0.17.4",
    "@vue/runtime-core": "^3.3.10",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.4.0",
    "commitizen": "^4.3.0",
    "commitlint": "^18.4.3",
    "concurrently": "^8.2.2",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.55.0",
    "eslint-define-config": "^2.0.0",
    "eslint-flat-config-viewer": "^0.1.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jsdom": "^23.0.1",
    "less": "^4.2.0",
    "pnpm-patch-i": "^0.2.1",
    "postcss": "^8.4.32",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "prettier": "^3.1.0",
    "stylelint": "^15.11.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^4.4.0",
    "stylelint-config-recommended": "^13.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-declaration-strict-value": "^1.9.2",
    "terser": "^5.25.0",
    "typescript": "~5.3.3",
    "unplugin-auto-import": "^0.17.2",
    "unplugin-svg-component": "^0.7.6",
    "unplugin-vue-components": "^0.26.0",
    "unplugin-vue-inspector": "^1.0.2",
    "unplugin-vue-macros": "^2.7.0",
    "unplugin-vue-router": "^0.7.0",
    "vite": "^5.0.12",
    "vite-plugin-vue-meta-layouts": "^0.3.3",
    "vitest": "^1.0.1",
    "volar-plugin-vue-router": "^0.2.0",
    "vue-tsc": "^1.8.25"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "config": {
    "cz-customizable": {
      "config": "./.cz-config.cjs"
    },
    "commitizen": {
      "path": "cz-customizable"
    }
  }
}
