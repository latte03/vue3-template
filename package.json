{
  "name": "vue3-template",
  "version": "0.1.3",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "concurrently \"pnpm:type-check\" \"pnpm:build-only\"",
    "preview": "vite preview",
    "test": "vitest",
    "test:unit": "vitest --environment jsdom --root src/",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "eslint-viewer": "@eslint/config-inspector",
    "lint": "eslint . --fix",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "cz": "git add . && cz",
    "bumpp": "bumpp"
  },
  "dependencies": {
    "@eslint/config-inspector": "^1.1.0",
    "@unocss/reset": "^66.1.4",
    "@vue-macros/reactivity-transform": "^3.0.0-beta.14",
    "@vueuse/core": "^13.3.0",
    "@vueuse/head": "^2.0.0",
    "alova": "^3.3.1",
    "axios": "^1.9.0",
    "bumpp": "^10.1.1",
    "fast-querystring": "^1.1.2",
    "lodash-es": "^4.17.21",
    "nprogress-v2": "^1.1.10",
    "pinia": "^3.0.3",
    "tdesign-icons-vue-next": "^0.3.6",
    "tdesign-vue-next": "^1.13.2",
    "unocss": "^66.1.4",
    "vue": "^3.5.16",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@plugin-web-update-notification/vite": "^2.0.0",
    "@sxzz/eslint-config": "^7.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.15.31",
    "@unocss/preset-rem-to-px": "^66.1.4",
    "@unocss/transformer-directives": "^66.1.4",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitejs/plugin-vue-jsx": "^4.2.0",
    "@vue-macros/volar": "^0.30.15",
    "@vue/runtime-core": "^3.5.16",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "commitizen": "^4.3.1",
    "commitlint": "^19.8.1",
    "concurrently": "^9.1.2",
    "cz-customizable": "^7.4.0",
    "eslint": "^9.28.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "jsdom": "^26.1.0",
    "less": "^4.3.0",
    "pnpm-patch-i": "^0.4.1",
    "postcss": "^8.5.4",
    "postcss-html": "^1.8.0",
    "postcss-less": "^6.0.0",
    "prettier": "^3.5.3",
    "stylelint": "^16.20.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^6.0.0",
    "stylelint-config-recommended": "^16.0.0",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-declaration-strict-value": "^1.10.11",
    "terser": "^5.42.0",
    "typescript": "~5.8.3",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-compression": "^0.0.6",
    "unplugin-info": "^1.2.2",
    "unplugin-svg-component": "^0.12.1",
    "unplugin-turbo-console": "^2.1.3",
    "unplugin-unused": "^0.5.1",
    "unplugin-vue-components": "^28.7.0",
    "unplugin-vue-inspector": "^2.3.1",
    "unplugin-vue-router": "^0.12.0",
    "vite": "^6.3.5",
    "vite-plugin-vue-devtools": "^7.7.6",
    "vite-plugin-vue-meta-layouts": "^0.5.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.3",
    "volar-plugin-vue-router": "^0.2.0",
    "vue-macros": "3.0.0-beta.14",
    "vue-tsc": "^2.2.10"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "config": {
    "cz-customizable": {
      "config": "./.cz-config.cjs"
    },
    "commitizen": {
      "path": "cz-customizable"
    }
  },
  "pnpm": {
    "patchedDependencies": {
      "nprogress-v2": "patches/nprogress-v2.patch"
    }
  }
}
