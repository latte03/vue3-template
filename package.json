{
  "name": "vue3-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "concurrently \"pnpm:type-check\" \"pnpm:build-only\"",
    "preview": "vite preview",
    "test": "vitest",
    "test:unit": "vitest --environment jsdom --root src/",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "cz": "git add . && cz"
  },
  "dependencies": {
    "@unocss/reset": "^0.57.2",
    "@vue-macros/reactivity-transform": "^0.3.23",
    "@vueuse/core": "^10.5.0",
    "@vueuse/head": "^2.0.0",
    "axios": "^1.6.0",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "tdesign-icons-vue-next": "^0.2.2",
    "tdesign-vue-next": "^1.6.8",
    "unocss": "^0.57.2",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.5.1",
    "@types/jsdom": "^21.1.5",
    "@types/node": "^20.9.0",
    "@types/nprogress": "^0.2.3",
    "@unocss/preset-rem-to-px": "^0.57.2",
    "@unocss/transformer-directives": "^0.57.2",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue-macros/volar": "^0.17.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/runtime-core": "^3.3.8",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "commitizen": "^4.3.0",
    "commitlint": "^18.2.0",
    "concurrently": "^8.2.2",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.53.0",
    "eslint-define-config": "^1.24.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-vue": "^9.18.1",
    "jsdom": "^22.1.0",
    "less": "^4.2.0",
    "pnpm-patch-i": "^0.2.1",
    "postcss": "^8.4.31",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "prettier": "^3.0.3",
    "stylelint": "^15.11.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-recommended": "^13.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-declaration-strict-value": "^1.9.2",
    "terser": "^5.24.0",
    "typescript": "~5.2.2",
    "unplugin-auto-import": "^0.16.7",
    "unplugin-svg-component": "^0.7.6",
    "unplugin-vue-components": "^0.25.2",
    "unplugin-vue-inspector": "^1.0.0",
    "unplugin-vue-macros": "^2.6.2",
    "unplugin-vue-router": "^0.7.0",
    "vite": "^4.5.0",
    "vite-plugin-vue-meta-layouts": "^0.3.1",
    "vitest": "^0.34.6",
    "volar-plugin-vue-router": "^0.2.0",
    "vue-tsc": "^1.8.22"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "config": {
    "cz-customizable": {
      "config": "./.cz-config.cjs"
    },
    "commitizen": {
      "path": "cz-customizable"
    }
  }
}
