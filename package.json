{
  "name": "vue3-template",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@^7.20",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "cz": "git add . && cz"
  },
  "dependencies": {
    "@unocss/reset": "^0.48.5",
    "@vue-macros/reactivity-transform": "^0.2.5",
    "@vueuse/core": "^9.13.0",
    "@vueuse/head": "^1.1.23",
    "axios": "^1.3.4",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.33",
    "tdesign-icons-vue-next": "^0.1.8",
    "tdesign-vue-next": "^1.2.1",
    "unocss": "^0.48.5",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.15.3",
    "@types/nprogress": "^0.2.0",
    "@unocss/preset-rem-to-px": "^0.50.6",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue-macros/volar": "^0.8.4",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/runtime-core": "^3.2.47",
    "@vue/test-utils": "^2.3.1",
    "@vue/tsconfig": "^0.1.3",
    "commitizen": "^4.3.0",
    "commitlint": "^17.5.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.36.0",
    "eslint-define-config": "^1.17.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-vue": "^9.9.0",
    "jsdom": "^20.0.3",
    "less": "^4.1.3",
    "npm-run-all": "^4.1.5",
    "pnpm-patch-i": "^0.0.0",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.8.5",
    "stylelint": "^15.3.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-recommended": "^10.0.1",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^30.0.1",
    "stylelint-declaration-strict-value": "^1.9.2",
    "stylelint-order": "^6.0.3",
    "typescript": "~4.7.4",
    "unplugin-auto-import": "^0.15.1",
    "unplugin-svg-component": "^0.5.1",
    "unplugin-vue-components": "^0.22.12",
    "unplugin-vue-inspector": "^0.2.3",
    "unplugin-vue-macros": "^1.10.1",
    "unplugin-vue-router": "^0.5.4",
    "vite": "^4.2.1",
    "vite-plugin-pages": "^0.28.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-layouts": "^0.8.0",
    "vitest": "^0.25.8",
    "volar-plugin-vue-router": "^0.0.2",
    "vue-tsc": "^1.2.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "config": {
    "cz-customizable": {
      "config": ".cz-config.cjs"
    },
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
