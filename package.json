{
  "name": "vue3-template",
  "version": "0.1.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "concurrently \"pnpm:type-check\" \"pnpm:build-only\"",
    "preview": "vite preview",
    "test": "vitest",
    "test:unit": "vitest --environment jsdom --root src/",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "eslint-viewer": "eslint-flat-config-viewer",
    "lint": "eslint . --fix",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "cz": "git add . && cz"
  },
  "dependencies": {
    "@unocss/reset": "^0.59.0",
    "@vue-macros/reactivity-transform": "^0.4.4",
    "@vueuse/core": "^10.9.0",
    "@vueuse/head": "^2.0.0",
    "alova": "^2.18.3",
    "axios": "^1.6.8",
    "fast-querystring": "^1.1.2",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "tdesign-icons-vue-next": "^0.2.2",
    "tdesign-vue-next": "^1.9.3",
    "unocss": "^0.59.0",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@plugin-web-update-notification/vite": "^1.7.1",
    "@sxzz/eslint-config": "^3.8.7",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.12.5",
    "@types/nprogress": "^0.2.3",
    "@unocss/preset-rem-to-px": "^0.59.0",
    "@unocss/transformer-directives": "^0.59.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue-macros/volar": "^0.18.18",
    "@vue/runtime-core": "^3.4.21",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.5.1",
    "commitizen": "^4.3.0",
    "commitlint": "^19.2.1",
    "concurrently": "^8.2.2",
    "cz-customizable": "^7.0.0",
    "eslint": "^9.0.0",
    "eslint-define-config": "^2.1.0",
    "eslint-flat-config-viewer": "^0.1.20",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "jsdom": "^24.0.0",
    "less": "^4.2.0",
    "pnpm-patch-i": "^0.3.3",
    "postcss": "^8.4.38",
    "postcss-html": "^1.6.0",
    "postcss-less": "^6.0.0",
    "prettier": "^3.2.5",
    "stylelint": "^16.3.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^5.0.0",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-declaration-strict-value": "^1.10.4",
    "terser": "^5.30.3",
    "typescript": "~5.4.4",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-compression": "^0.0.6",
    "unplugin-info": "^1.1.0",
    "unplugin-svg-component": "^0.8.0",
    "unplugin-vue-components": "^0.26.0",
    "unplugin-vue-inspector": "^1.0.2",
    "unplugin-vue-macros": "^2.9.0",
    "unplugin-vue-router": "^0.8.5",
    "vite": "^5.2.8",
    "vite-plugin-vue-meta-layouts": "^0.4.2",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.4.0",
    "volar-plugin-vue-router": "^0.2.0",
    "vue-tsc": "^2.0.11"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "config": {
    "cz-customizable": {
      "config": "./.cz-config.cjs"
    },
    "commitizen": {
      "path": "cz-customizable"
    }
  }
}
